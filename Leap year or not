def is_leap_year(year):
    # Write your code here. 
    # Don't change the function name.
    leap = True
    if year % 4 == 0 and year % 100 == 0 and year % 400 == 0:
        leap
    else:
        leap = False
    return leap
print(is_leap_year(2000))
